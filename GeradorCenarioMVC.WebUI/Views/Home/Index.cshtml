@model GeradorCenarioMVC.WebUI.ViewModels.RegisterViewModel
@{
	Layout = "_HomeLayout";
}


<section id="slider" class="slider-element include-header min-vh-75" style="background: url( /images/landing/landing2.jpg) center; overflow: visible;">
	<div class="slider-inner flex-column justify-content-end">
		<div class="container" style="z-index: 1;">

			<div class="row align-items-end justify-content-end h-100">
				<div class="form-widget col-lg-4" data-loader="button">
					<div class="form-result"></div>

					<form asp-controller="Account" id="formRegister" asp-action="Register" method="post" class="landing-wide-form dark mb-0">
						<div class="row">
							<div class="col-12 form-group mb-4">
								<div class="heading-block mb-0 border-bottom-0">
									<h2>Cadastre-se</h2>
									<span>Faça Aqui Rapidamente</span>
								</div>
							</div>
							<div class="col-12 form-group">
								<input asp-for="FistName" id="firstName" class="form-control required form-control-lg not-dark" value="" placeholder="Seu Primeiro Nome*" />
							</div>
							<div class="col-12 form-group">
								<input asp-for="LastName" id="lastName" class="form-control required form-control-lg not-dark" value="" placeholder="Seu Último Nome*" />
							</div>
							<div class="col-12 form-group">
								<input asp-for="Email" id="email" class="form-control required form-control-lg not-dark" value="" placeholder="Seu Email*" />
							</div>
							<div class="col-12 form-group">
								<input asp-for="Password" id="password" class="form-control required form-control-lg not-dark" value="" placeholder="Escolha a Senha*" />
							</div>
							<div class="col-12 form-group">
								<input asp-for="ConfirmPassword" class="form-control required form-control-lg not-dark" value="" placeholder="Confirme a Senha*" />
							</div>
							<div class="col-12 form-group mb-0">
								<button class="btn btn-lg btn-danger w-100 m-0" id="butCadastro" @*value="submit"*@ type="button">FAÇA SEU CADASTRO</button>
							</div>

							<p>
								<a asp-controller="Account" asp-action="Login">Se já tem cadastro clique aqui</a>
							</p>

						</div>
						<input type="hidden" name="prefix" value="template-landing5-">
					</form>
				</div>
			</div>

		</div>

		<div class="promo promo-dark promo-full landing-promo p-5 position-relative position-lg-absolute w-100" style="top: auto; bottom: 0;">
			<div class="container">
				<div class="row align-items-center">
					<div class="col-12 col-lg-8">
						<h3>Beautiful Landing Page designs waiting for you inside <i class="bi-circle-fill-arrow-right position-relative" style="top:2px;"></i></h3>
						<span>You'll love our beautiful &amp; interactive builder that makes your build process easier &amp; fun.</span>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>

