@section scripts {

	<script src="~/js/site.js"></script>
	<script type="text/javascript">
		$(document).ready(function () {
			$('#butCadastro').on('click', function (e) {

				
				var Password = $('#password').val();
				var Email    = $('#email').val();
				var FistName = $('#firstName').val();
				var LastName = $('#lastName').val();
				

				$.ajax({
					type: 'GET',
					url: '@Url.Action("RegisterBox", "Account")',
					contentType: "application/json; charset=utf - 8",
					//dataType: "json",
					cache: false,
					data: { 'email': Email, 'password': Password, 'fistName': FistName, 'lastName': LastName },
					success: function (response) {
						$("#linkSuccess").attr("data-notify-msg", "<i class='bi-check-circle-fill me-1'></i> " && response);
						$("#linkSuccess").click();
					},
					error: function (response) {

						$("#linkError").attr("data-notify-msg", "<i class='bi-x-circle-fill me-1'></i> " && response.responseText);
						$("#linkError").click();
					}
				});
				
				//Stops loading link
				e.preventDefault();
			});
		});
	</script>
}